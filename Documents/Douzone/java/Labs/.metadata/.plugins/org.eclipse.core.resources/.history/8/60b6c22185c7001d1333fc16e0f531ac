package Quiz;

public class Statement_Quiz {

	public static void main(String[] args) {
		// 1번
		for (int i=0; i<26; i++) {
			System.out.print((char)('A'+i));
		}
		System.out.println();
		
		//2번
		for (int z=1; z<=100; z++) {
			System.out.print(" " + z);
			if (z%10 == 0) {
				System.out.println();
			}
		}

	}

}
